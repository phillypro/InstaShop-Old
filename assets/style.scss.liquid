// custom transition
		.ngslide.in {
    -webkit-animation:ngscaleUp 0.15s both linear;
	-moz-animation:ngscaleUp 0.15s both linear;
	animation:ngscaleUp 0.15s both linear;   

            z-index: 996;
		}
		

		.ngslide.out {
    -webkit-animation:ngslideOutLeft 0.15s both ease-in;
	-moz-animation:ngslideOutLeft 0.15s both ease-in;
	animation:ngslideOutLeft 0.15s both ease-in;   

              z-index: 995;
		}


		.ngslide.in.reverse {

            -webkit-animation:ngslideInLeft 0.2s both linear;
	-moz-animation:ngslideInLeft 0.2s both linear;
	animation:ngslideInLeft 0.2s both linear;  
             z-index: 996;
		}
		

		.ngslide.out.reverse {
          -webkit-animation-direction:reverse;
          animation-direction:reverse;
             -webkit-animation:ngscaleDown 0.2s both linear;
	-moz-animation:ngscaleDown 0.2s both linear;
	animation:ngscaleDown 0.2s both linear;   
             z-index: 995;
		}



/* slide in from the bottom */
@keyframes ngscaleUp {
	from 		{ transform: translateX(100%);   }
}
@-moz-keyframes ngscaleUp {
	from 		{ -moz-transform: translateX(100%); }
}
@-webkit-keyframes ngscaleUp {
	from 		{ -webkit-transform: translateX(100%); }
}

/* slide in from the bottom */
@keyframes ngslideOutLeft {
	to 		{ transform: translateX(-10%); }
}
@-moz-keyframes ngslideOutLeft {	
	to 		{ -moz-transform: translateX(-10%); }
}
@-webkit-keyframes ngslideOutLeft {
	to 		{ -webkit-transform: translateX(-10%); }
}




/* slide in from the bottom */
@keyframes ngslideInLeft {
  from		{ transform: translate(-10%, 0%); }
	to 		{ transform: translate(0%, 0%); }
}
@-moz-keyframes ngslideInLeft {	
    from	{ -moz-transform: translate(-10%, 0%); }
	to 		{ -moz-transform: translate(0%, 0%); }
}
@-webkit-keyframes ngslideInLeft {
    from	{ -webkit-transform: translate(-10%, 0%); }
	to 		{ -webkit-transform: translate(0%, 0%); }
}

/* slide in from the bottom */
@keyframes ngscaleDown {

  	to	    { transform: translate(100%, 0%); }
}
@-moz-keyframes ngscaleDown {

  	to		{ -moz-transform: translate(100%, 0%); }
}
@-webkit-keyframes ngscaleDown {

  	to 		{ -webkit-transform: translate(100%, 0%); }
}

