@import url('https://fonts.googleapis.com/css?family=Open+Sans:400,700|Lato|Oswald:500|Poppins:800&display=swap');

@font-face {
    font-family: 'HelveticaNeueMedium';
    src: url('{{'HelveticaNeueMedium.eot' | asset_url }}');
    src: url('{{'HelveticaNeueMedium.eot' | asset_url }}') format('embedded-opentype'),
         url('{{'HelveticaNeueMedium.woff2' | asset_url }}') format('woff2'),
         url('{{'HelveticaNeueMedium.woff' | asset_url }}') format('woff'),
         url('{{'HelveticaNeueMedium.ttf' | asset_url }}') format('truetype'),
         url('{{'HelveticaNeueMedium.svg' | asset_url }}#HelveticaNeueMedium') format('svg');
}


