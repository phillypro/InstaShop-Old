jQuery('.share-button').off('click').on('click', function(e) {
		var parent = jQuery(this).parent();
		if(parent.hasClass('open')) {
			parent.removeClass('open');
		}else{
			parent.addClass('open');
		}
	});
	  
	 jQuery('.write a').off('click').on('click', function(e) {
		var parent = jQuery(this).parent();
		if(!parent.hasClass('open')) {
			parent.addClass('open');
			setTimeout(function() {
				parent.removeClass('open');
			}, 2000);
		}
	});
	  
	  
	jQuery('.yotpo-helpful a').off('click').on('click', function(e) {
		var countdiv = jQuery(this).children('span');
		var count = countdiv.text();
		var sibling = jQuery(this).siblings().children('span');
		if(count > 0) {
			countdiv.text('0');
			sibling.text('0');
		}else{
			countdiv.text('1');
			sibling.text('0');
		}
